<h1>Listing fotocopia</h1>

<table>
  <thead>
    <tr>
      <th>Titulo</th>
      <th>Originais</th>
      <th>Quantidade</th>
      <th>Data</th>
      <th>Justificativa</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @fotocopia.each do |fotocopium| %>
      <tr>
        <td><%= fotocopium.titulo %></td>
        <td><%= fotocopium.originais %></td>
        <td><%= fotocopium.quantidade %></td>
        <td><%= fotocopium.data %></td>
        <td><%= fotocopium.justificativa %></td>
        <td><%= link_to 'Show', fotocopium %></td>
        <td><%= link_to 'Edit', edit_fotocopium_path(fotocopium) %></td>
        <td><%= link_to 'Destroy', fotocopium, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Fotocopium', new_fotocopium_path %>
